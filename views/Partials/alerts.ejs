<% if(alerts.error) {%>
	<% alerts.error.forEach(function(message) {%>
	<div class="alert alert-danger fixed-bottom"><%= message %></div>
	<%});%>
<%}%>

<% if(alerts.success) {%>
	<% alerts.success.forEach(function(message) {%>
	<div class="alert alert-success fixed-bottom"><%= message %></div>
	<%});%>
<%}%>